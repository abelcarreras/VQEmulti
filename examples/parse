var3=$( grep "expectation" ${1} | cut -b14-50 )
var2=$( grep "variance" ${1} | cut -b10-50)
#var1=$( grep "depth" ${1} | cut -b7-50)
var1=$( grep "n_operators:" kill | cut -b13-50)

perl -le '@in = map { [split] } @ARGV; for $i ( 0..$#{ $in[0] } ) { print join "  ", map { $in[$_][$i] } 0..$#in; }' "$var1" "$var2" "$var3"

